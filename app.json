{
  "expo": {
    "scheme": "yeetapp",
    "name": "Yeet",
    "slug": "Yeet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/UXMaterials/AppIcon.png",
    "userInterfaceStyle": "light",
    "androidStatusBar": {
      "backgroundColor": "#fff0",
      "translucent": true
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/UXMaterials/adaptiveAppIcon.png",
      },
      "package": "com.jamesapol.Yeet"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
